@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>

<div class="form-horizontal" id="project-form">
    <input type="hidden" data-bind="value: id" value="@ViewBag.Id" name="Id" />
    <div class="form-group">
        <label class="col-md-2 control-label">Enhancement Request</label>
        <div class="col-md-10">
            <input name="EnhancementRequestId" data-bind="value: enhancementRequestId" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-2 control-label">Manager</label>
        <div class="col-md-10">
            <input name="ManagerId" data-bind="value: managerId" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-2 control-label">Start</label>
        <div class="col-md-10">
            <input name="Start" data-bind="value: start" />
        </div>
    </div>
    <div class="form-group">
        <label class="col-md-2 control-label">Finish</label>
        <div class="col-md-10">
            <input name="Finish" data-bind="value: finish" />
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <button class="btn btn-primary" data-bind="enabled: submitEnabled, click: submit" type="button">Submit</button>
        </div>
    </div>
</div>

@section scripts {
<script src="~/Scripts/Developer/Project/edit.js"></script>
}